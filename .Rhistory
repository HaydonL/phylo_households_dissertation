SGb1_lpdf <- function(alpha, a, m){
a_m <- numeric(m)
s <- numeric(m - 1)
for (i in 1:m){
a_m[i] <- alpha + i - 1
}
for (j in 1:(m - 1)){
s[j] <- (-1)^(a + j) * j^(a - 2) * log(j) / (gamma(j) * gamma(m - j))
}
density <- (a - 1) * log(alpha) - prod(a_m) - log(sum(s))
return(density)
}
